<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Set Document Command"
            categoryId="org.kathrynhuxtable.eclipse.plugin.setmacdocument.commands.category"
            id="org.kathrynhuxtable.eclipse.plugin.setmacdocument.commands.setDocumentCommand">
      </command>
   </extension>
   
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.kathrynhuxtable.eclipse.plugin.setmacdocument.commands.setDocumentCommand"
            class="org.kathrynhuxtable.eclipse.plugin.setmacdocument.handlers.SetMacDocumentHandler">
      </handler>
   </extension>
   
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.kathrynhuxtable.eclipse.plugin.setmacdocument.commands.setDocumentCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>

</plugin>
